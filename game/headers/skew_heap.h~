#ifndef SKEW_HEAP
#define SKEW_HEAP
typedef struct Node{
  void *data;
  int key;
  struct Node *left;
  struct Node *right;
}Node;
#define SWAP_NODES(left_heap, right_heap)/
Node *holder = left_heap;/
left_heap = right_heap; /
right_heap = holder;
       



Node *sh_imerge(Node *left, Node *right);

void sh_add_node(Node *root, Node *node);

Node *sh_extract_min(Node *root);

#endif
